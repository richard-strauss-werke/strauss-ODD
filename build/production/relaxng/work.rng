   <define name="mei_date">
      <element name="date" ns="http://www.music-encoding.org/ns/mei">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A string identifying a point in time or the time period between two such points.</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="mei_model.textphraseLike"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="schema.work-date-dateHeuristics1-constraint-1">
            <rule context="mei:date">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="(@notbefore gt @notafter) and not(starts-with(@notbefore, '-'))">value of notBefore greater than notAfter</sch:report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="schema.work-date-dateHeuristics3-constraint-2">
            <rule context="mei:date">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="(@startdate or @enddate) and (@isodate or @notafter or @notbefore)">cooccurence of date and duration attributes</sch:report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="schema.work-date-dateHeuristics4-constraint-3">
            <rule context="mei:date">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0"
                           xmlns:rng="http://relaxng.org/ns/structure/1.0"
                           xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                           xmlns:s="http://www.ascc.net/xml/schematron"
                           xmlns:xi="http://www.w3.org/2001/XInclude"
                           test="@isodate and (@notafter or @notbefore)">cooccurence of exact and uncertain date attributes</sch:report>
            </rule>
         </pattern>
         <ref name="mei_att.calendared.attributes"/>
         <ref name="mei_att.datable.attributes"/>
         <ref name="mei_att.edit.attributes"/>
         <empty/>
      </element>
   </define>
